{
  "name": "coinkrazy-casino-mobile",
  "version": "1.0.0",
  "description": "CoinKrazy Casino Mobile App",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:web && npm run build:mobile",
    "build:web": "vite build",
    "build:mobile": "cap sync",
    "build:android": "npm run build:web && cap sync android && cap build android",
    "build:ios": "npm run build:web && cap sync ios && cap build ios",
    "dev:android": "cap run android --livereload --external",
    "dev:ios": "cap run ios --livereload --external",
    "sync": "cap sync",
    "sync:android": "cap sync android",
    "sync:ios": "cap sync ios",
    "open:android": "cap open android",
    "open:ios": "cap open ios",
    "add:android": "cap add android",
    "add:ios": "cap add ios",
    "copy": "cap copy",
    "update": "cap update",
    "doctor": "cap doctor",
    "clean": "cap clean",
    "serve": "vite preview --host"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/app": "^5.0.0",
    "@capacitor/camera": "^5.0.0",
    "@capacitor/clipboard": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/device": "^5.0.0",
    "@capacitor/geolocation": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/local-notifications": "^5.0.0",
    "@capacitor/network": "^5.0.0",
    "@capacitor/push-notifications": "^5.0.0",
    "@capacitor/share": "^5.0.0",
    "@capacitor/splash-screen": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@capacitor/storage": "^5.0.0",
    "@capacitor/toast": "^5.0.0"
  },
  "devDependencies": {
